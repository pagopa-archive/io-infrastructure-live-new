<policies>
    <inbound>
        <base />
        <set-backend-service base-url="{{io-fn-cgnoperatorsearch-url}}" />
        <set-header name="x-functions-key" exists-action="override">
            <value>{{io-fn-cgnoperatorsearch-key}}</value>
        </set-header>
        <set-header name="x-from-external" exists-action="override">
            <value>true</value>
        </set-header>
        <cors>
            <allowed-origins>
                <origin>*</origin>
            </allowed-origins>
            <allowed-methods>
                <method>*</method>
            </allowed-methods>
            <allowed-headers>
                <header>*</header>
            </allowed-headers>
            <expose-headers>
                <header>*</header>
            </expose-headers>
        </cors>
    </inbound>
    <outbound>
        <base />
    </outbound>
</policies>